    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "react-svg": "^14.1.3",
    "sass": "^1.44.0"
  },
  "scripts": {
    "start": "PORT=3105 react-scripts start",
    "test": "react-scripts test --coverage",
